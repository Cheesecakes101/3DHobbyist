{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://threedhobbyist-backend.onrender.com/api/:path*"
    },
    {
      "source": "/attached_assets/:path*",
      "destination": "https://threedhobbyist-backend.onrender.com/attached_assets/:path*"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
